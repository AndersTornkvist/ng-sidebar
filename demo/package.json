    "ng-sidebar": "file:../",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.1.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.0",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
