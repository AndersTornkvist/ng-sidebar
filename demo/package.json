{
  "name": "ng-sidebar-demo",
  "version": "0.0.0",
  "description": "Demo of ng-sidebar.",
  "repository": {
    "type": "git",
    "url": "https://github.com/arkon/ng-sidebar.git"
  },
  "author": "Eugene Cheung",
  "license": "MIT",
  "scripts": {
